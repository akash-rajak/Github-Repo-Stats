{

    "manifest_version": 3,

    "name": "GitHub Repo Stats",

    "version": "0.0.2",

    "description": "A chrome extension using HTML, CSS and JavaScript to fetch the Github Repo Stats.",

    "action":{
        "default_popup": "index.html",
        "default_title": "Github Repo Stats"
    },

    "icons": {
        "48": "Images/icon.png"
    },

    "permissions": [
        "webRequest"
    ]
}
  