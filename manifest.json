{

    "manifest_version": 3,

    "name": "GitHub Repo Stats",

    "version": "1.1.0",

    "description": "A dynamic Chrome Extension to fetch the Github Repo Stats.",

    "action":{
        "default_popup": "index.html",
        "default_title": "Github Repo Stats"
    },

    "icons": {
        "48": "Images/icon.png"
    },

    "permissions": [
        "webRequest"
    ]
}
  